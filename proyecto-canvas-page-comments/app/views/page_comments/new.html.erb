<%= form_for @page_comment do |f| %>
    <div>
        <%= f.label :message %>
        <%= f.text_area :message %>
    </div>
    <div>
        <%= f.label :page_id %>
        <%= f.number_field :page_id%>
    </div>
    <div>
        <%= f.label :page_type %>
        <%= f.text_field :page_type%>
    </div>
     <div>
        <%= f.label :user_id %>
        <%= f.number_field :user_id %>
    </div>
    <div>
        <%= f.submit %>
    </div>
<% end %>